{"ast":null,"code":"import * as echarts from 'echarts';\nimport { mapGetters } from 'vuex';\nexport default {\n  data() {\n    return {};\n  },\n  name: 'LineChart',\n  props: ['echarts_info'],\n  computed: {\n    ...mapGetters(['echarts_list']),\n    Option() {\n      let that = this;\n      let option = {\n        xAxis: {\n          type: 'value'\n          // data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']\n        },\n\n        yAxis: {\n          type: 'category'\n          // data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']\n        },\n\n        series: [{\n          data: this.echarts_list.shap,\n          // data:[120, 200, 150, 80, 70, 110, 130],\n          type: 'bar',\n          showBackground: true,\n          backgroundStyle: {\n            color: 'rgba(180, 180, 180, 0.2)'\n          }\n        }]\n      };\n      return option;\n    }\n  },\n  mounted() {\n    this.getEchartsList();\n    this.initEcharts();\n  },\n  methods: {\n    // 获取用户信息\n    async getEchartsList(user_id) {\n      try {\n        await this.$store.dispatch('getEchartsList', 1);\n      } catch (e) {\n        console.warn(e.message);\n      }\n    },\n    async initEcharts() {\n      this.data.Option.series[0].data = this.echarts_list.shap;\n      // 初始化echarts实例\n      let lineCharts = echarts.init(this.$refs.lineCharts);\n      // 配置数据\n      lineCharts.setOption(this.option);\n    }\n  }\n};","map":{"version":3,"names":["echarts","mapGetters","data","name","props","computed","Option","that","option","xAxis","type","yAxis","series","echarts_list","shap","showBackground","backgroundStyle","color","mounted","getEchartsList","initEcharts","methods","user_id","$store","dispatch","e","console","warn","message","lineCharts","init","$refs","setOption"],"sources":["src/views/Index/Home/ShadowChart/index.vue"],"sourcesContent":["<template>\r\n  <!-- shap图 -->\r\n\r\n  <div class=\"LineChart maxWH\" ref=\"lineCharts\">\r\n   \r\n  </div> \r\n   <!-- <div>{{echarts_list.shap}