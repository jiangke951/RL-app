{"ast":null,"code":"import * as echarts from 'echarts';\nexport default {\n  name: 'LineChart',\n  props: ['echarts_info'],\n  mounted() {\n    // 初始化echarts实例\n    let lineCharts = echarts.init(this.$refs.lineCharts);\n\n    // 配置数据\n    lineCharts.setOption({\n      grid: {\n        left: \"15\",\n        containLabel: true\n      },\n      // legend: {\n      //   data: ['Profit', 'Expenses', 'Income']\n      // },\n      grid: {\n        left: '3%',\n        right: '4%',\n        bottom: '3%',\n        containLabel: true\n      },\n      xAxis: [\n      //x轴数据设置\n      {\n        type: \"value\",\n        name: \"人数\",\n        min: 0,\n        axisLabel: {\n          formatter: \"{value} \"\n        },\n        splitLine: {\n          show: false\n        },\n        axisLine: {\n          //这是y轴文字颜色\n          lineStyle: {\n            color: \"#65C6E7\"\n          }\n        }\n      }],\n      yAxis: [{\n        type: \"category\",\n        offset: 0,\n        axisLine: {\n          //这是x轴文字颜色\n          lineStyle: {\n            color: \"#65C6E7\"\n          }\n        },\n        data: this.palceDatas,\n        axisPointer: {\n          type: \"shadow\"\n        },\n        axisTick: {\n          show: false\n        }\n      }],\n      series: [{\n        data: this.palceNum,\n        type: \"bar\",\n        barWidth: 22,\n        label: {\n          normal: {\n            show: true\n            // position: \"right\",\n          }\n        },\n\n        itemStyle: {\n          //通常情况下：\n          normal: {\n            //每个柱子的颜色即为colorList数组里的每一项，如果柱子数目多于colorList的长度，则柱子颜色循环使用该数组\n            color: function (params) {\n              var colorList = [\"#855BD7\", \"#4B6FB8\", \"#0F6E43\", \"#B69C2B\", \"#2588BB\", \"#BB612D\", \"#1D9394\", \"#C24545\"];\n              return colorList[params.dataIndex];\n            }\n          }\n        }\n      }]\n    });\n  }\n};","map":{"version":3,"names":["echarts","name","props","mounted","lineCharts","init","$refs","setOption","grid","left","containLabel","right","bottom","xAxis","type","min","axisLabel","formatter","splitLine","show","axisLine","lineStyle","color","yAxis","offset","data","palceDatas","axisPointer","axisTick","series","palceNum","barWidth","label","normal","itemStyle","params","colorList","dataIndex"],"sources":["src/views/Index/Home/ShadowChart/index.vue"],"sourcesContent":["<template>\r\n  <div class=\"LineChart maxWH\" ref=\"lineCharts\">\r\n  </div>\r\n \r\n   \r\n  \r\n  \r\n     \r\n     \r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport * as echarts from 'echarts'\r\n\r\n\r\nexport default {\r\n  name: 'LineChart',\r\n  props: ['echarts_info'],\r\n  mounted() {\r\n    \r\n      // 初始化echarts实例\r\n      let lineCharts = echarts.init(this.$refs.lineCharts)\r\n\r\n\r\n      // 配置数据\r\n      lineCharts.setOption({\r\n        grid: {\r\n      left: \"15\",\r\n      containLabel: true,\r\n    },\r\n// legend: {\r\n//   data: ['Profit', 'Expenses', 'Income']\r\n// },\r\ngrid: {\r\n  left: '3%',\r\n  right: '4%',\r\n  bottom: '3%',\r\n  containLabel: true\r\n},\r\nxAxis: [\r\n      //x轴数据设置\r\n      {\r\n        type: \"value\",\r\n        name: \"人数\",\r\n        min: 0,       \r\n        axisLabel: {\r\n          formatter: \"{value} \",\r\n        },\r\n        splitLine: {\r\n          show: false,\r\n        },\r\n        axisLine: {\r\n          //这是y轴文字颜色\r\n          lineStyle: {\r\n            color: \"#65C6E7\",\r\n          },\r\n        },\r\n      },\r\n    ],\r\n    yAxis: [\r\n      {\r\n        type: \"category\",\r\n        offset: 0,\r\n        axisLine: {\r\n          //这是x轴文字颜色\r\n          lineStyle: {\r\n            color: \"#65C6E7\",\r\n          },\r\n        },\r\n        data: this.palceDatas,\r\n        axisPointer: {\r\n          type: \"shadow\",\r\n        },\r\n        axisTick: {\r\n          show: false,\r\n        },\r\n      },\r\n    ],\r\n    series: [\r\n      {\r\n        data: this.palceNum,\r\n        type: \"bar\",\r\n        barWidth: 22,\r\n        label: {\r\n          normal: {\r\n            show: true,\r\n            // position: \"right\",\r\n          },\r\n        },\r\n        itemStyle: {\r\n          //通常情况下：\r\n          normal: {\r\n            //每个柱子的颜色即为colorList数组里的每一项，如果柱子数目多于colorList的长度，则柱子颜色循环使用该数组\r\n            color: function (params) {\r\n              var colorList = [\r\n                \"#855BD7\",\r\n                \"#4B6FB8\",\r\n                \"#0F6E43\",\r\n                \"#B69C2B\",\r\n                \"#2588BB\",\r\n                \"#BB612D\",\r\n                \"#1D9394\",\r\n                \"#C24545\",\r\n              ];\r\n              return colorList[params.dataIndex];\r\n            },\r\n          },\r\n        },\r\n      },\r\n    ],\r\n\r\n              })\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n// img{\r\n// width:562; \r\n// height:274px;\r\n// position: relative;\r\n\r\n// }\r\n.parent {\r\nwidth: 562px;\r\nheight: 600px;\r\ndisplay: flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\nimg {\r\nmax-width: 100%;\r\nmax-height: 100%;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AAcA,YAAAA,OAAA;AAGA;EACAC,IAAA;EACAC,KAAA;EACAC,QAAA;IAEA;IACA,IAAAC,UAAA,GAAAJ,OAAA,CAAAK,IAAA,MAAAC,KAAA,CAAAF,UAAA;;IAGA;IACAA,UAAA,CAAAG,SAAA;MACAC,IAAA;QACAC,IAAA;QACAC,YAAA;MACA;MACA;MACA;MACA;MACAF,IAAA;QACAC,IAAA;QACAE,KAAA;QACAC,MAAA;QACAF,YAAA;MACA;MACAG,KAAA;MACA;MACA;QACAC,IAAA;QACAb,IAAA;QACAc,GAAA;QACAC,SAAA;UACAC,SAAA;QACA;QACAC,SAAA;UACAC,IAAA;QACA;QACAC,QAAA;UACA;UACAC,SAAA;YACAC,KAAA;UACA;QACA;MACA,EACA;MACAC,KAAA,GACA;QACAT,IAAA;QACAU,MAAA;QACAJ,QAAA;UACA;UACAC,SAAA;YACAC,KAAA;UACA;QACA;QACAG,IAAA,OAAAC,UAAA;QACAC,WAAA;UACAb,IAAA;QACA;QACAc,QAAA;UACAT,IAAA;QACA;MACA,EACA;MACAU,MAAA,GACA;QACAJ,IAAA,OAAAK,QAAA;QACAhB,IAAA;QACAiB,QAAA;QACAC,KAAA;UACAC,MAAA;YACAd,IAAA;YACA;UACA;QACA;;QACAe,SAAA;UACA;UACAD,MAAA;YACA;YACAX,KAAA,WAAAA,CAAAa,MAAA;cACA,IAAAC,SAAA,IACA,WACA,WACA,WACA,WACA,WACA